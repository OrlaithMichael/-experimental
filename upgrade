os.execute("wget -f https://raw.githubusercontent.com/OrlaithMichael/-experimental/master/upgrade-list.lua")
os.execute("wget -f https://raw.githubusercontent.com/OrlaithMichael/-experimental/master/download-list")
os.execute("wget -f https://raw.githubusercontent.com/OrlaithMichael/-experimental/master/instructions.lua")


--[[
local file = io.open("download-list")

for line in file:lines() do
    os.execute("wget -f https://raw.githubusercontent.com/OrlaithMichael/-experimental/master/"..line)
end

local upgrade = require("upgrade-list")

for key, value in pairs(upgrade.download) do
    os.execute("wget -f https://raw.githubusercontent.com/OrlaithMichael/-experimental/master/"..upgrade.download[key])
end

for key, value in pairs(upgrade.rm) do
    os.execute ("rm -v "..upgrade.rm[key])
end

os.execute ("rm -v upgrade-list.lua")
]]

local upgrade = require("instructions")
upgrade.up

os.execute ("rm -v instructions.lua")