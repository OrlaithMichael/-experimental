os.execute("wget -f https://raw.githubusercontent.com/OrlaithMichael/-experimental/master/upgrade-list")

local upgrade = require("upgrade-list")

for key, value in pairs(upgrade.list.download) do
    os.execute("wget -f https://raw.githubusercontent.com/OrlaithMichael/-experimental/master/"..upgrade.list.download[key])
end

for key, value in pairs(upgrade.list.rm) do
    os.execute ("rm -v "..upgrade.list.rm[key])
end

os.execute ("rm -v upgrade-list")